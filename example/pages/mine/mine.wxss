/**index.wxss**/

.container{
  padding: 0;
  padding-top: 100rpx;
  font-size: 28rpx;
}

.title .img{
  width: 100%;
  height: 120rpx;
}

.cont{
  padding: 0 30rpx;
}

.cont-list{
  margin-bottom: 20rpx;
}

.cont-item{
  padding-bottom: 40rpx;
}

.cont-item .label{
  text-align: left;
  font-size: 28rpx;
  font-weight: 600;
  height: 100rpx;
  line-height: 100rpx;
  border-top: 1rpx solid #e8e8e8;
  border-bottom: 1rpx solid #e8e8e8;
}

.cont-item .list {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.cont-item .item{
  text-align: center;
}

.cont-item .item-img{
  width: 200rpx;
  height: 200rpx;
  border-radius: 100rpx;
  margin: 20rpx 0;
}

.cont-item .item-img2{
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx 0;
}



