.counter-box{
    height: 40rpx;
    width: 146rpx;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    color: #777;
    font-size: 30rpx;
}
.counter-btn{
    width:36rpx;
    height: 36rpx;
    border: 1px solid #ccc;
    font-size: 28rpx;
    line-height: 36rpx;
    text-align: center;
}
.counter-num{
    border: 1px solid #ccc;
    border-width: 1rpx 0;
    margin:0 -1rpx 0 -1prx;
    flex:1;
    text-align: center;
    height: 36rpx;
    line-height: 36rpx;
    font-size: 26rpx;
}
.btn-unvaliable{
    background-color:#eee; 
}
